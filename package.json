{
    "name": "wiev",
    "version": "1.4.3",
    "description": "Provides a view interface for the browser",
    "main": "src/index.js",
    "type": "module",
    "scripts": {
        "build": "node build/build.js",
        "semantic-release": "semantic-release"
    },
    "files": [
        "/dist/*",
        "/src/*"
    ],
    "devDependencies": {
        "@semantic-release/commit-analyzer": "^9.0.2",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^8.0.7",
        "@semantic-release/npm": "^9.0.1",
        "esbuild": "^0.13.12",
        "esbuild-plugin-clear": "^1.0.1",
        "esbuild-plugin-time": "^1.0.0",
        "js-translator": "^5.0.1",
        "semantic-release": "^19.0.5",
        "semantic-release-cli": "^5.4.4"
    },
    "peerDependencies": {
        "js-translator": "^5.0.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DasRed/wiev.git"
    },
    "author": "Marco Starker",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DasRed/wiev/issues"
    },
    "homepage": "https://github.com/DasRed/wiev#readme",
    "keywords": [
        "wiev",
        "view",
        "browser"
    ],
    "dependencies": {
        "eventemitter0": "^3.0.0"
    },
    "release": {
        "branches": [
            "main"
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            [
                "@semantic-release/npm",
                {
                    "npmPublish": false
                }
            ],
            [
                "@semantic-release/git",
                {
                    "assets": [
                        "package.json",
                        "package-lock.json"
                    ]
                }
            ],
            "@semantic-release/github"
        ]
    }
}
